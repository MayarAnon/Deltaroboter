[Unit]
Description=Deltaroboter Service
After=multi-user-target network-online.target mosquitto.service
Requires= network-online.target mosquitto.service

[Service]
Type=simple
ExecStart=/bin/bash /home/pi/Deltaroboter/Bash/start.sh
WorkingDirectory=/home/pi/Deltaroboter/Bash
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
